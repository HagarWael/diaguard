echo *.swp >> .gitignore
echo .DS_Store >> .gitignore
echo .atom/ >> .gitignore
echo .buildlog/ >> .gitignore
echo .history >> .gitignore
echo .svn/ >> .gitignore
echo migrate_working_dir/ >> .gitignore
echo. >> .gitignore
